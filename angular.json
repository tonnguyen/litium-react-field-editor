{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "react-field-editor": {
      "root": "projects/react-field-editor",
      "sourceRoot": "projects/react-field-editor/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/react-field-editor/tsconfig.lib.json",
            "project": "projects/react-field-editor/ng-package.json"
          }
        }
      }
    }
  },
  "defaultProject": "react-field-editor"
}